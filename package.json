{
  "name": "ez-web-audio",
  "type": "module",
  "version": "0.0.0-notready",
  "private": false,
  "description": "Making the Web Audio API super EZ since 2024.",
  "author": {
    "name": "Seth Brasile",
    "email": "seth.brasile@gmail.com",
    "url": "https://github.com/sethbrasile"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sethbrasile/ez-web-audio.git"
  },
  "keywords": [
    "audio",
    "web",
    "audio-api",
    "web-audio",
    "web-audio-api",
    "audio-context",
    "audio-node",
    "synthesis",
    "oscillator"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite -c vite.docs.config.js",
    "build": "tsc && vite build",
    "build:docs": "tsc && vite build -c vite.docs.config.js",
    "preview": "vite preview -c vite.docs.config.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.26.0",
    "@nx-js/observer-util": "^4.2.2",
    "@types/prismjs": "^1.26.4",
    "eslint": "9.5.0",
    "happy-dom": "^15.7.0",
    "prismjs": "^1.29.0",
    "standardized-audio-context": "^25.3.77",
    "standardized-audio-context-mock": "^9.7.8",
    "typescript": "^5.5.3",
    "vite": "^5.4.0",
    "vite-plugin-dts": "^4.2.1",
    "vite-plugin-prismjs": "^0.0.11",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.0.5"
  }
}
